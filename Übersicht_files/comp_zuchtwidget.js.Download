(function( comp_zuchtwidget, $ ) {
    
    function init() {
        // Click header, image
        $("img.breedimage, div#zuchtwidget > .header").on("click", function() {
            location.href="/zuchtkalender/index";
        });                       
        
        $(".nextstepcontainer > span").on("click", function() {
            var id = $(this).attr("data-id");
            location.href="/zuchtkalender/update?id="+id;
        });
    }
    
    init();
    
}( window.comp_zuchtwidget = window.comp_zuchtwidget || {}, jQuery ));