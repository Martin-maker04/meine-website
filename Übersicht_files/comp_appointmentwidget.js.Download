(function( comp_appointmentwidget, $ ) {
    
    function init() {
        // Click header, image
        $("div#appointmentwidget > .header").on("click", function() {
            location.href="/termine/index";
        }); 
        
        $(".nextappcontainer > span").on("click", function() {
            const id = $(this).attr("data-id");
            const link = $(this).attr("data-link");
            location.href=link+"?id="+id;
        });
    }
    
    init();
    
}( window.comp_appointmentwidget = window.comp_appointmentwidget || {}, jQuery ));